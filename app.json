{
  "expo": {
    "name": "IV Scaner",
    "slug": "iv-scaner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/IV.png",
    "scheme": "ivscaner",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/IV.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.ignaciov92.ivscaner"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/IV.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/IV.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Permiso para la cámara",
          "microphonePermission": "Permiso para el micrófono",
          "recordAudioAndroid": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7d76eb70-f053-4346-b84e-438265a58325"
      }
    }
  }
}
